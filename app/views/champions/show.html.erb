<h1>Champion: <%= @champion_name %></h1>

<% if @players.present? %>
  <table>
    <thead>
      <tr>
        <th>Score</th>
        <th>Nickname</th>
        <th>Win Rate</th>
        <th>Tier</th>
      </tr>
    </thead>
    <tbody>
      <% @players.each do |player| %>
        <tr>
          <td><%= player["score"] %></td>
          <td><%= player["nickname"] %></td>
          <td><%= player["winrate"] %>%</td>
          <td><%= player["tier"] %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% else %>
  <p>No players found for this champion.</p>
<% end %>
